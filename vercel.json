{
  "version": 2,
  "builds": [
    {
      "src": "vercel_build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/web"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(?!.*\\.(?:js|css|html|json|png|jpg|jpeg|gif|svg|ico|ttf|otf|woff|woff2)$)(.*)",
      "dest": "/index.html"
    }
  ]
}